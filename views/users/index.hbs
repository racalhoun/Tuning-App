<div>
    <h1>Users:</h1>
    <div>
        <a href="/users/new">Add a New User</a>
    </div>
    {{#each users}}
       <div>
           {{this.name}} - {{this.phoneNumber}} - {{this.email}} - {{this.userName}} - {{this.password}} - {{this.stable}}
           <a href="/users/{{this._id}}">{{this.name}}Info</a>
           <a href="/users/{{this._id}}/cars">{{this.name}}'s Cars</a>
       </div>
       {{/each}}
</div>